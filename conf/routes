GET   /                     controllers.LandingPage.get()
GET   /assets/*file         controllers.Assets.at(path="/public", file)
GET   /user/                controllers.UserIndex.get()
POST  /user/                controllers.UserIndex.post()
POST  /.logout              controllers.UserIndex.deleteToken()
POST  /.auth                controllers.UserIndex.sendToken()
GET   /.auth                controllers.UserIndex.authControl()
GET   /resource/            controllers.ResourceIndex.list(q: String ?= "")
GET   /resource/:id         controllers.ResourceIndex.read(id: String)
POST  /resource/            controllers.ResourceIndex.create()
POST  /resource/:id         controllers.ResourceIndex.update(id: String)
GET   /aggregation/         controllers.AggregationIndex.list()
GET   /country/:id          controllers.CountryIndex.read(id: String)
GET   /:page                controllers.StaticPage.get(page: String)
