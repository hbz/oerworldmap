        "duration": {
          "type": "string"
        },
        "hasMonetaryValue": {
          "type": "string"
        },
        "description": {
          "description": "The description of the grant",
          "$ref": "#/definitions/LocalizedString"
        },
        "name": {
          "$ref": "#/definitions/LocalizedString"
        },
        "alternateName": {
          "$ref": "#/definitions/LocalizedString"
        },
        "isAwardedBy": {
          "description": "An organization the grant is awarded by.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Organization"
          }
        },
        "hasAwardDate": {
          "$ref": "#/definitions/iso-8601-date"
        },
        "mentionedIn": {
          "description": "An article that mentions the grant.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "mainEntityOf": {
          "description": "An article that has the grant as its main entity.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "keywords": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "funds": {
          "description": "The action being funded",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Action"
          }
        }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    },
