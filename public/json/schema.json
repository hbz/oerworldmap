      "additionalProperties": false
    },
    "Event": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "title": "Event",
      "description": "An event that is being held for/by members of the OER community.",
      "type": "object",
      "properties": {
        "@context": {
          "type": "string",
          "format": "uri"
        },
        "@id": {
          "description": "The event's unique identifier",
          "type": "string",
          "format": "uri"
        },
        "@type": {
          "type": "string",
          "enum": [
            "Event"
          ]
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "organizer": {
          "description": "The event's organizer ",
          "type": "array",
          "items": {
            "type": "object",
            "oneOf": [
              {
                "$ref": "#/definitions/Person"
              },
              {
                "$ref": "#/definitions/Organization"
              },
              {
                "$ref": "#/definitions/Action"
              }
            ]
          }
        },
        "performer": {
          "description": "People presenting, leading a workshop etc. at the event",
          "type": "array",
          "items": {
                "$ref": "#/definitions/Person"
          }
        },
        "name": {
          "$ref": "#/definitions/LocalizedString"
        },
        "alternateName": {
          "$ref": "#/definitions/LocalizedString"
        },
        "description": {
          "$ref": "#/definitions/LocalizedString"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "url": {
          "type": "string",
          "format": "uri"
        },
        "sameAs": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          }
        },
        "recordedIn": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "uri"
          }
        },
        "inLanguage": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "startDate": {
          "$ref": "#/definitions/iso-8601-date"
        },
        "endDate": {
          "$ref": "#/definitions/iso-8601-date"
        },
        "location": {
          "$ref": "#/definitions/Place"
        },
        "mentionedIn": {
          "description": "An article that mentions the event.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "mainEntityOf": {
          "description": "An article that mentions the event as main topic.",
          "type": "array",
          "items": {
            "$ref": "#/definitions/Article"
          }
        },
        "hashtag": {
          "type": "string"
        },
      "keywords": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "about": {
        "type": "array",
        "items": {
          "allOf": [
            {
              "$ref": "#/definitions/Concept"
            },
            {
              "properties": {
                "inScheme": {
                  "type": "object",
                  "properties": {
                    "@id": {
                      "enum": [
                        "https://w3id.org/class/esc/scheme"
                      ]
                    }
                  }
                }
              }
            }
          ]
        }
      },
      "comment": {
        "description": "A comment about the Event",
        "type": "array",
        "items": {
          "$ref": "#/definitions/Comment"
        }
      }
      },
      "required": [
        "name"
      ],
      "additionalProperties": false
    },
    "Grant": {
      "$schema": "http://json-schema.org/draft-04/schema#",
      "title": "Grant",
      "description": "A grant awarded to a project",
      "type": "object",
      "properties": {
        "@type": {
          "type": "string",
          "enum": [
            "Grant"
          ]
