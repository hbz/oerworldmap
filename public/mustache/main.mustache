    <script>
        var user = {{json user}};
        var permissions = {{json permissions}};
        var user_location = {{json userLocation}};
        var embed = {{json embed}};
    </script>

</head>
<body class="">

<header class="clearfix" id="page-header">

    <div class="visible-print">
        <h1>OER W<i class="fa fa-globe"></i>RLD MAP</h1>
    </div>

    <div class="hidden-print hidden-mobile">

        <nav class="main">
            <ul>
                <li><h1><a href="/" title="Home" {{#if @root.embed}}target="_blank"{{/if}}>OER W<i class="fa fa-globe"></i>RLD MAP</a></h1></li>
                {{#unless @root.embed}}
                    {{#each sections.primary}}
                        <li><a href="/{{[@key]}}" title="{{ . }}">{{ . }}</a></li>
                    {{/each}}
                    <li><a href="https://oerworldmap.wordpress.com/" target="_blank">{{i18n "main.blog"}}</a></li>
                {{/unless}}
            </ul>
        </nav>

        <nav class="social{{#if @root.embed}} hidden{{/if}}">
            <ul>

                <li><a href="https://www.facebook.com/oerworldmap" title="Facebook" target="_blank"><i class="fa fa-facebook-official"></i></a></li>
                <li><a href="https://twitter.com/oerworldmap" title="Twitter" target="_blank"><i class="fa fa-twitter"></i></a></li>
            </ul>
        </nav>

        <nav class="user{{#if @root.embed}} hidden{{/if}}">
            <ul>

                {{#if user}}

                    <li class="dropdown">
                        <div class="a" type="button" data-toggle="dropdown" title="{{ username }}">
                            <i class="fa fa-user"></i>
                        </div>
                        <ul class="dropdown-menu pull-right">
                            <li><a href="/resource/{{user.[@id]}}"><i class="fa fa-fw fa-user"></i> {{i18n "main.myProfile"}}</a></li>
                            <li><a href="/user/password"><i class="fa fa-fw fa-lock"></i> {{i18n "main.changePassword"}}</a></li>
                            <li><a href="#" data-behaviour="logout"><i class="fa fa-fw fa-sign-out"></i> {{i18n "main.logout"}}</a></li>
                        </ul>
                    </li>

                {{else}}

                    <li><a href="/user/register">{{i18n "login"}}</a></li>

                {{/if}}

            </ul>
        </nav>

        {{#unless user}}

            <h2 class="slogan"><a href="/contribute" {{#if @root.embed}}target="_blank"{{/if}}>{{i18n "main.joinUsMessage"}}</a></h2>

        {{/unless}}

        <div class="hidden" id="global-statistics">
            <script type="application/ld+json" id="json-aggregations">
                {{json scope.globalAggregation}}
            </script>
            <datalist id="keywords">
            {{#scope.keywordAggregation.[about.keywords].buckets }}
                <option value="{{key}}"></option>
            {{/scope.keywordAggregation.[about.keywords].buckets}}
            </datalist>
        </div>

    </div>

    <div class="visible-mobile">

        <nav class="main-mobile">
            <button data-toggle="dropdown"><i class="fa fa-bars"></i></button>
            <ul class="dropdown-menu">
                <li><a href="/">{{i18n "main.map"}}</a></li>
                <li><a href="/contribute">{{i18n "contribute"}}</a></li>
                <li><a href="/FAQ">{{i18n "main.faq"}}</a></li>
                <li><a href="/about">{{i18n "main.about"}}</a></li>
                <li><a href="https://oerworldmap.wordpress.com/" target="_blank">{{i18n "main.blog"}}</a></li>
                <li><a href="/user/newsletter">{{i18n "main.newsletter"}}</a></li>
                <li><a href="https://www.facebook.com/oerworldmap" target="_blank">Facebook</a></li>
                <li><a href="https://twitter.com/oerworldmap" target="_blank">Twitter</a></li>
                <li><a href="/imprint">{{i18n "main.imprintPrivacy"}}</a></li>
            </ul>
        </nav>

        <nav class="user-mobile">

            {{#if user}}

                <button data-toggle="dropdown"><i class="fa fa-user"></i></button>
                <ul class="dropdown-menu">
                    <li><a href="/resource/{{user.[@id]}}">{{i18n "main.myProfile"}}</a></li>
                    <li><a href="/user/password">{{i18n "main.changePassword"}}</a></li>
                    <li><a href="#" data-behaviour="logout">{{i18n "main.logout"}}</a></li>
                </ul>

            {{else}}

                <a class="button" href="/user/register"><i class="fa fa-user"></i></a>

            {{/if}}

        </nav>

    </div>

</header>

<main>
    {{> (lookup . 'template') }}
</main>

<footer id="page-footer" class="container container-static-page color-scheme-text">
    <nav class="inner">
        <ul>
            <li class="hidden-mobile"><a href="/imprint" title="Imprint"{{#if @root.embed}}target="_blank"{{/if}}>{{i18n "main.imprintPrivacy"}}</a></li>
        </ul>
    </nav>
</footer>

