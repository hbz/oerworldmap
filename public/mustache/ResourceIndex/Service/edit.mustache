<fieldset>

    <legend>Service</legend>

    {{input "@type" "Service" readonly="readonly"}}

    {{input "@id" [@id] readonly="readonly"}}

    {{> FormElements/LocalizedInput.mustache property=name key="name" title="Name"}}

    {{> FormElements/LocalizedInput.mustache property=alternateName key="alternateName" title="Alternate Name"}}

    {{> ResourceIndex/ServiceChannel/edit.mustache property=availableChannel key="availableChannel"}}

    {{> FormElements/LocalizedTextarea.mustache property=description key="description" title="Description"}}

    {{> FormElements/Input.mustache property=startDate key="startDate" title="Start Date" multiple=false type="text"}}

    {{> FormElements/Input.mustache property=email key="email" title="Email" multiple=false type="email"}}

    {{> FormElements/Input.mustache property=sameAs key="sameAs" title="Same as" multiple=true type="url"}}

    {{> FormElements/Input.mustache property=license key="license" title="License" multiple=true type="text"}}

    {{> FormElements/Input.mustache property=image key="image" title="Image" multiple=false type="url"}}

    {{> ResourceIndex/Place/edit.mustache property=location key="location"}}

    {{> FormElements/Id.mustache property=provider key="provider" title="Provider" multiple=true lookup="/resource/?filter.about.@type=Organization"}}

    {{> FormElements/Id.mustache property=about key="about" title="Subject" multiple=true lookup="/resource/?filter.about.@type=Concept"}}

    {{> FormElements/Id.mustache property=audience key="audience" title="Audience" multiple=true lookup="/resource/?filter.about.@type=Concept"}}

    {{> FormElements/Id.mustache property=memberOf key="memberOf" title="Member of" multiple=true lookup="/resource/?filter.about.@type=Organization"}}

    {{> FormElements/Input.mustache property=serviceType key="serviceType" title="Service Type" multiple=false type="text"}}

    {{> FormElements/Id.mustache property=mainEntityOf key="mainEntityOf" title="Main Entity of" multiple=true lookup="/resource/?filter.about.@type=Article"}}

    {{> FormElements/Id.mustache property=mentionedIn key="mentionedIn" title="Mentioned in" multiple=true lookup="/resource/?filter.about.@type=Article"}}

    {{> FormElements/Id.mustache property=instrumentIn key="instrumentIn" title="Instrument in" multiple=true lookup="/resource/?filter.about.@type=Action"}}

    {{> FormElements/Id.mustache property=resultOf key="resultOf" title="Result of" multiple=true lookup="/resource/?filter.about.@type=Action"}}

    {{> FormElements/Id.mustache property=member key="member" title="Member" multiple=true lookup="/resource/"}}

</fieldset>
