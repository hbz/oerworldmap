<div role="complementary" class="color-scheme-map column large" data-view="map"></div>
<div role="main" class="column small">
  {{#scope.countryAggregation}}
    <table class="table" about="#users-by-country">
      <script type="application/ld+json">
        {{json .}}
      </script>
      <thead>
        <tr>
          <th>Country</th>
          <th>Users</th>
          <th>Organizations</th>
          <th>Stories</th>
          <th>Champions</th>
        </tr>
      </thead>
      <tbody>
      {{#by_country.buckets}}
        <tr about="#{{key}}">
          <td><a href="/country/{{key}}">{{key}}</a></td>
          {{#by_type.buckets}}
            <td>{{doc_count}}</td>
          {{/by_type.buckets}}
        </tr>
      {{/by_country.buckets}}
      </tbody>
    </table>
  {{/scope.countryAggregation}}
  <div class="pie-chart">
    {{pieChart scope.typeAggregation.[about.@type] width=300 height=300}}
  </div>
</div>
